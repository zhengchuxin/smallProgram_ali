.S_pannel {
  margin: 5px 9px;
  background-color: #fff;
}

.go_link {
  background-position: right 12px bottom 5px;
}

.S_pannel .S_title {
  font-size: 16px;
  color: #051b28;
  font-weight: 600;
  padding-top: 12px;
}

.S_pannel .one_cell {
  padding: 12px;
  border-bottom: 1px solid #eee;
}

.S_pannel .cell_last {
  border-bottom: 0;
}

.S_pannel .gard_two {
  display: table;
  width: 100%;
  height: 24px;
  line-height: 24px;
}

.S_pannel .gard_two .gard_two_left {
  display: table-cell;
  font-size: 13px;
  width: 100px;
  color: #666;
}

.S_pannel .gard_two .gard_two_right {
  display: table-cell;
  font-size: 13px;
  color: #051b28;
}

.S_pannel .S_title img:first-child {
  width: 29px;
  height: 25px;
  vertical-align: middle;
  margin-top: -5px;
  margin-right: 4px;
}
